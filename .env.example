# GitHub API Token (for data collection)
# Create a personal access token at: https://github.com/settings/tokens
# Required scopes: public_repo, read:org, read:user
GITHUB_TOKEN=your_github_token_here

# Multiple GitHub tokens for higher throughput (optional)
# Comma-separated list of tokens for 2-3x API rate limits
# GITHUB_TOKENS=ghp_token1,ghp_token2,ghp_token3

# GitLab API Token (optional)
GITLAB_TOKEN=your_gitlab_token_here

# Rate limiting configuration
MAX_REQUESTS_PER_HOUR=5000
REQUEST_TIMEOUT=30

# Data collection settings
PARALLEL_REQUESTS=5
CACHE_ENABLED=true
CACHE_DIR=.cache

# Analysis configuration
RANDOM_SEED=42
CONFIDENCE_LEVEL=0.95
ALPHA=0.05

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/collector.log
